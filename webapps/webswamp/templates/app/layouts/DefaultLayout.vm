## SWAMP Workflow Administration and Management Platform

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" 
	"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <link rel="SHORTCUT ICON" href="$ui.image("favicon")" title="Favicon">
    <title>$page.Title</title>
    <meta http-equiv="Content-Type" content="text/html; utf-8">
    #foreach($metaTag in $page.MetaTags.keySet())
        <meta name="$metaTag" content="$page.MetaTags.get($metaTag)">
    #end

    <link rel="stylesheet" href="$ui.getCommonStylecss()" type="text/css" 
		title="SWAMP common stylesheet">

    <link rel="stylesheet" href="$ui.getStylecss()" type="text/css" 
		title="SWAMP skin stylesheet">

    #foreach( $styleSheet in $page.StyleSheets )
        <link rel="stylesheet" href="$styleSheet.Url"
        #if($styleSheet.Type != "" ) type="$styleSheet.Type" #end
        #if($styleSheet.Media != "") media="$styleSheet.Media" #end
        #if($styleSheet.Title != "") title="$styleSheet.Title" #end
        >
    #end

</head>

## set focus on login page
#if ($data.getScreenTemplate().indexOf("Login") >= 0)
<body bgcolor="#ffffff" text="#000000" onLoad="document.loginform.username.focus()">
#else 
<body bgcolor="#ffffff" text="#000000">
#end

<script language="JavaScript" src="/webswamp/resources/ui/skins/common/wz_tooltip.js" type="text/javascript"></script>
<script language="JavaScript" src="/webswamp/resources/ui/skins/common/prototype.js" type="text/javascript"></script>
<script language="JavaScript" src="/webswamp/resources/ui/skins/common/bugzillaPopup.js" type="text/javascript"></script>

<script language="JavaScript">
<!-- Begin
   function submitForm(s) {
   s.value = "Please wait";
   s.disabled = true;
   return true;
}
// End -->
</script>


<table width="100%" border="0">
  <tr>
    <td colspan="2">
    $navigation.setTemplate("/DefaultTop.vm")
    </td>
  </tr>
  <tr>
    <td width="20" align="left" valign="top">
      $navigation.setTemplate("/Menu.vm")
    </td>
    <td align="left" valign="top" height="100%">

      <table width="100%" cellspacing="0" cellpadding="0" border="0" height="100%">
          <tr>
              <td width="100%" valign="top" height="21">
                <table cellpadding="0" cellspacing="0" border="0" width="100%">     
                  <tr>
                    <td align="left" valign="top" width="7"><img src="$ui.image("bigboxcornertop")" alt=" " border="0"></td>
                    <td width="100%" style="background-image:url($ui.image("bigboxbordertop")); background-position:top; background-repeat: repeat-x;" nowrap="nowrap">
						<img src="$ui.image("clearpix")" width="6" height="18" alt=" " border="0"></td>
                  </tr>
                </table>
              </td>
            </tr>
            

            <tr>
              <td align="left" valign="top">
                
                <table cellpadding="0" cellspacing="0" border="0" width="100%" height="100%">
                  <tr height="100%">
					<!-- Cell with vertical Line -->
                    <td align="left" height="100%" bgcolor="#006666">
						<img src="$ui.image("clearpix")" height="1" width="1" alt="" border="0">
					</td>
                    <!-- Spacer Cell -->
					<td align="left" width="20">
						<img src="$ui.image("clearpix")" height="1" width="20" alt="" border="0">
					</td>
                    <!-- Content Cell -->
					<td align="left" valign="top" width="100%">
						
						#parse("StatusMessage.vm")
						
						#parse("WorkflowReadResult.vm")
						
						$screen_placeholder

					</td>
					<!-- Spacer Cell -->
					<td align="left" width="20">
						<img src="$ui.image("clearpix")" height="1" width="20" alt="" border="0">
					</td>
                  </tr>
                </table>
                
              </td>
            </tr>
            
            
            <tr>
              <td align="left" valign="top">
                <table cellpadding="0" cellspacing="0" border="0" width="100%">     
                  <tr>
                    <td align="left" valign="top" width="7">
                    <img src="$ui.image("bigboxcornerbottom")" alt=" " border="0"></td>
                    <td align="left" valign="top" width="100%" 
                    style="background-image:url($ui.image("bigboxborderbottom")); background-position:bottom; background-repeat: repeat-x;"><img src="$ui.image("clearpix")" width="1" height="7" alt=" " border="0"></td>
                  </tr>
                </table>
              </td>
            </tr>
          </table>

    </td>
  </tr>
  <tr>
    <td colspan="2">
      $navigation.setTemplate("/DefaultBottom.vm")
    </td>
  </tr>
</table>


</body>
</html>
